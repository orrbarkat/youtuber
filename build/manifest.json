{
  "manifest_version": 2,

  "name": "youtube mix ban artists",
  "description": "This extension will ban annoying artists from your playlist",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "storage",
   "tabs",
   "*://www.youtube.com/*"
   ]
}
